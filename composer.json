{
    "name": "sbarale/laravel-api-consumer",
    "description": "Awesome Laravel Api Consumer",
    "keywords": [
        "laravel api consumer",
        "laravel",
        "api consumer"
    ],
    "homepage": "https://github.com/sbarale/larave-api-consumer",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "kitetail/zttp": "^0.3.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.*"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "BlackBits\\ApiConsumer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlackBits\\ApiConsumer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlackBits\\ApiConsumer\\ApiConsumerServiceProvider"
            ]
        }
    }
}
